{
  "name": "mytmp_nuxt_heroku",
  "version": "2.0.0",
  "description": "my template for nuxt, vuetify, express, mongodb, mongoose",
  "author": "testuser",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "devmon": "nodemon --watch api --exec \"nuxt\"",
    "dev-debug": "node --inspect node_modules/nuxt/bin/nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "test-api": "export NODE_ENV=test || SET \"NODE_ENV=test\" && mocha api/**/**.test.js",
    "test-api-debug": "export NODE_ENV=test || SET \"NODE_ENV=test\" && mocha --inspect-brk  api/**/**.test.js",
    "heroku-postbuild": "npm run build",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lintfix": "eslint --fix --ext .js,.vue --ignore-path .gitignore ."
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.0.0",
    "body-parser": "^1.18.3",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "jsonwebtoken": "^8.3.0",
    "lodash": "^4.17.10",
    "mongodb": "^3.1.3",
    "mongoose": "^5.2.8",
    "nuxt": "2.1",
    "vuetify": "^1.2.9"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "cross-env": "^5.0.1",
    "eslint": "^5.7.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-vue": "^4.7.1",
    "expect": "^23.5.0",
    "mocha": "^5.2.0",
    "prettier": "^1.14.3",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "supertest": "^3.1.0"
  }
}
